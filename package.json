{
  "name": "nest-typescript-prisma-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "prettier": "prettier \"**/*.{ts,tsx,css,scss,json}\" --config ./.prettierrc.js",
    "prettier:fix": "prettier --write \"**/*.{js,ts,json,md}\"",
    "script:lint": "bash -e ./scripts/lint.sh",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ts": "tsc --noEmit --skipLibCheck -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "prismadb": "prisma generate",
    "prismamigrate": "prisma migrate dev",
    "prismamigrate:local": "NODE_ENV=development npx prisma migrate dev",
    "seed": "ts-node src/prisma/seed.ts",
    "seed:prod": "node dist/prisma/seed.js"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.17",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^10.4.17",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^10.4.15",
    "@prisma/client": "^6.2.1",
    "@types/multer": "^1.4.12",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.5",
    "dotenv": "^16.4.7",
    "express-ejs-layouts": "^2.5.1",
    "express-winston": "^4.2.0",
    "google-auth-library": "^9.15.1",
    "handlebars": "^4.7.8",
    "lru-cache": "^11.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "nestjs-i18n": "^10.5.0",
    "nestjs-seeder": "^0.3.2",
    "nodemailer": "^6.10.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.15",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.0.0",
    "prisma": "^6.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
